# Check manifest will not automatically add these two files:
include .dockerignore
include .editorconfig
include .tx/config
prune docs/_build
recursive-include sdsc_deployer *.po *.pot *.mo

# added by check_manifest.py
include *.rst
include *.sh
include *.txt
include Dockerfile
include LICENSE
include Makefile
include NOTICE
include babel.ini
include docker-compose.yml
include pytest.ini
include uwsgi.ini
recursive-include docker *
recursive-include docs *.bat
recursive-include docs *.py
recursive-include docs *.rst
recursive-include docs *.txt
recursive-include docs Makefile
recursive-include examples *.py
recursive-include examples *.sh
recursive-include sdsc_deployer *.css
recursive-include sdsc_deployer *.gif
recursive-include sdsc_deployer *.html
recursive-include sdsc_deployer *.ico
recursive-include sdsc_deployer *.js
recursive-include sdsc_deployer *.json
recursive-include sdsc_deployer *.map
recursive-include sdsc_deployer *.png
recursive-include sdsc_deployer *.ttf
recursive-include sdsc_deployer *.yaml
recursive-include tests *.py
